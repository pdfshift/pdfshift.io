        <link rel="mask-icon" href="/images/favicons/safari-pinned-tab.svg" color="#623e98">
        <meta name="apple-mobile-web-app-title" content="PDFShift">
        <meta name="application-name" content="PDFShift">
        <meta name="msapplication-TileColor" content="#623e98">
        <meta name="msapplication-TileImage" content="/images/favicons/mstile-144x144.png">
        <link rel="manifest" href="/site.webmanifest">
        <meta name="msapplication-config" content="/browserconfig.xml">
        <meta name="theme-color" content="#ffffff">


        <meta property="og:type"        content="website">
        <meta property="og:url"         content="https://pdfshift.io">
        <meta property="og:title"       content="{{ seo_title | safe }}" />
        <meta property="og:description" content="{{ seo_description | safe }}" />
        <meta property="og:image"       content="https://pdfshift.io/images/slogan.png">
        <meta property="og:image"       content="https://pdfshift.io/images/favicons/android-chrome-256x256.png">

        <meta name="twitter:site"        content="@pdfshift">
        <meta name="twitter:creator"     content="@cx42net">
        <meta name="twitter:card"        content="summary_large_image">
