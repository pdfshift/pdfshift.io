        <script>
        if (window.PDFShift === undefined) {
            window.PDFShift = {
                '_ready': [],
                'onReady': function (callback) {window.PDFShift._ready.push(callback);}
            };
        }

        if (window.document.location.hostname === '127.0.0.1') {
            window.PDFShift.api_url = 'http://127.0.0.1:5000/v2/';
        } else {
            window.PDFShift.api_url = 'https://api.pdfshift.io/v2/';
        }
        </script>
